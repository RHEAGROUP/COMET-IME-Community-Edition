<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFrameworks>net6.0-windows;net48</TargetFrameworks>
    <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\</SolutionDir>
    <AssemblyTitle>CDP4ShellDialogs</AssemblyTitle>
    <Company>RHEA System S.A</Company>
    <Product>CDP4ShellDialogs</Product>
    <Description>COMET Shell Dialogs</Description>
    <Copyright>Copyright © RHEA System S.A</Copyright>
    <AssemblyVersion>9.2.0.5</AssemblyVersion>
    <FileVersion>9.2.0.5</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
	<UseWPF>true</UseWPF>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x64'">
    <DebugType>full</DebugType>
    <CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x64'">
    <DebugType>pdbonly</DebugType>
    <CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CDP4Common-CE" Version="9.0.0-750-PR236" />
    <PackageReference Include="CDP4Dal-CE" Version="9.0.0-750-PR236" />
    <PackageReference Include="CDP4JsonSerializer-CE" Version="9.0.0-750-PR236" />
    <PackageReference Include="CommonServiceLocator" Version="2.0.5" />
    <PackageReference Include="ReactiveUI" Version="18.3.1" />
    <PackageReference Include="System.ComponentModel.Composition" Version="6.0.0" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.39" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="NLog" Version="4.7.10" />
    <PackageReference Include="ReactiveUI.WPF" Version="18.3.1" />
    <PackageReference Include="System.Xml.XmlSerializer" Version="4.3.0" />
  </ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net48'">
		<PackageReference Include="DevExpress.CodeParser" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Diagram" Version="20.2.11" />
		<PackageReference Include="DevExpress.Data" Version="20.2.11" />
		<PackageReference Include="DevExpress.Data.Desktop" Version="20.2.11" />
		<PackageReference Include="DevExpress.Images" Version="20.2.11" />
		<PackageReference Include="DevExpress.MVVM" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Core" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Grid" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Grid.Core" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Layout" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.PropertyGrid" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.SpellChecker" Version="20.2.11" />
		<PackageReference Include="DevExpress.Wpf.Themes.Seven" Version="20.2.11" />
		<!--
			Those references have to be added manually, see:
			https://stackoverflow.com/questions/62074980/
		-->
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Security" />
		<Reference Include="System.Web" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0-windows'">
		<PackageReference Include="DevExpress.WindowsDesktop.CodeParser" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.Data" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.DataDesktop" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.Images" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.Mvvm" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.Wpf" Version="20.2.11" />
		<PackageReference Include="DevExpress.WindowsDesktop.Wpf.Themes.Seven" Version="20.2.11" />
	</ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\CDP4Composition\CDP4Composition.csproj" />
  </ItemGroup>
</Project>